Hi! Here's the events we have available

<ul>
  <% @events.each do |event| %>
    <li>
        Creator: <%= User.find(event.creator_id).username %>
        <div>
          Title: <%= event.title %>
        </div>
        <div>
          Details: <%= event.body %>
        </div>
    </li>
  <% end %>
</ul>



<%= button_to "Create Event", new_event_url, method: :get %><br />
<%= button_to "Sign out", destroy_user_session_url, method: :delete %>
<%= link_to "Sign in", new_user_session_url %>
